<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AppLockView">

        <attr name="textSize" format="float" />
        <attr name="textColor" format="integer" />
        <attr name="keyAmount" format="integer" />
        <attr name="keyboardBackgroundColor" format="integer" />
        <attr name="pinCodeColor" format="integer" />
    </declare-styleable>

    <declare-styleable name="SlidingUpDrawer">

        <attr name="peekHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LottoDigitsEditText">
        <attr name="digits" format="integer" />
        <attr name="fontSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PatternUnlockView">
        <attr name="pressZoneSize" format="dimension" />
        <attr name="pinRadius" format="dimension" />
        <attr name="pinColor" format="color" />
        <attr name="pressedColor" format="color" />
        <attr name="pressPinRadius" format="dimension" />
        <attr name="lineWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="BadgeTextView">
        <attr name="test" format="string" />
    </declare-styleable>

    <declare-styleable name="SwipeViewLayout">
        <attr name="swipeMode" format="enum">
            <enum name="behind" value="0" />
            <enum name="beside" value="1" />
        </attr>
    </declare-styleable>

</resources>